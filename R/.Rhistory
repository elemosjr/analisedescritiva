boxplot_continuas <- function(df,
grid = TRUE,
axis = FALSE,
coord_flip = FALSE,
col = "black",
fill = "gray40",
position = "dodge2",
outlier.color = NULL,
outlier.fill = NULL,
outlier.shape = 19,
outlier.size = 1.5,
notch = FALSE)
{
cols <- names(df)
lista <- list()
vars_id <- c()
for(i in 1:length(cols))
{
if(is.numeric(df[[i]][[1]]) | is.double(df[[i]][[1]]))
{
vars_id <- c(vars_id, i)
}
}
if(is.null(vars_id))
{
stop("O dataframe não tem variáveis continuas")
} else
{
for(i in vars_id)
{
p <- ggplot(df, aes_string(y = names(df)[i]))+
geom_boxplot(col = col,
fill = col,
position = position,
outlier.color = outlier.color,
outlier.fill = outlier.fill,
outlier.shape = outlier.shape,
outlier.size = outlier.size,
notch = notch)
lista[[names(df)[i]]]
}
}
if(grid == TRUE)
{
grid_list(lista)
} else
{
return(lista)
}
}
